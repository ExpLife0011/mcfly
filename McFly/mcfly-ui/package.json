{
  "name": "mcfly",
  "version": "0.0.1",
  "description": "Web UI for mcfly web app",
  "main": "index.js",
  "repository": "http://github.com/tysmithnet/mcfly.git",
  "author": "@tysmithnet",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "webpack-dev-server --config webpack.dev.config.js --hot --inline --host 0.0.0.0",
    "test": "webpack --config webpack.dev.config.js && jest",
    "storybook": "start-storybook -p 80",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "d3-force": "1.1.0",
    "esvalidate": "0.1.3",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-redux": "5.0.7",
    "react-redux-router": "0.0.14",
    "uuid": "3.2.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.4.5",
    "@storybook/addon-info": "3.4.5",
    "@storybook/addon-links": "3.4.5",
    "@storybook/addon-storyshots": "3.4.5",
    "@storybook/addons": "3.4.5",
    "@storybook/react": "3.4.5",
    "@types/d3-force": "1.1.1",
    "@types/enzyme": "3.1.10",
    "@types/jest": "22.2.3",
    "@types/react": "16.3.14",
    "@types/react-dom": "16.0.5",
    "@types/react-redux": "6.0.0",
    "@types/react-router-redux": "^5.0.14",
    "@types/react-test-renderer": "16.0.1",
    "@types/storybook__addon-actions": "3.0.3",
    "@types/storybook__addon-links": "3.3.0",
    "@types/storybook__addon-storyshots": "3.4.1",
    "@types/storybook__react": "3.0.7",
    "@types/uuid": "3.4.3",
    "awesome-typescript-loader": "5.0.0",
    "babel": "6.23.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.4",
    "babel-runtime": "6.26.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "jest": "22.4.4",
    "react-docgen-typescript-webpack-plugin": "1.1.0",
    "react-test-renderer": "16.3.2",
    "source-map-loader": "0.2.3",
    "spdx-license-ids": "3.0.0",
    "ts-jest": "22.4.6",
    "ts-loader": "3.5.0",
    "typescript": "2.8.3",
    "webpack": "4.9.1",
    "webpack-cli": "2.1.4"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "enableTsDiagnostics": true
      }
    },
    "setupFiles": [
      "<rootDir>/test-shim.js",
      "<rootDir>/test-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test-preprocessor.js"
    },
    "testMatch": [
      "**/*.spec.(ts|tsx|js)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
